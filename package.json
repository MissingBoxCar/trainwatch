{
  "name": "trainwatch",
  "version": "1.0.0",
  "description": "Watches Trains",
  "scripts": {
    "start": "parcel serve index.html",
    "build": "parcel build index.html",
    "build-report": "parcel build index.html --detailed-report 20",
    "format": "prettier --write .",
    "analyzer-build": "parcel build index.html --reporter @parcel/reporter-bundle-analyzer",
    "analyzer": "npm run -s analyzer-build && node analyzer.js && parcel serve parcel-bundle-reports/filtered.html --open"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fontsource/roboto": "^4.5.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@parcel/config-default": "^2.3.2",
    "@parcel/packager-xml": "^2.3.2",
    "@parcel/reporter-bundle-analyzer": "^2.3.2",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@welldone-software/why-did-you-render": "^7.0.1",
    "parcel": "^2.3.2",
    "process": "^0.11.10"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "last 2 versions",
      "Firefox ESR",
      "not IE > 0",
      "not IE_mob > 0",
      "not bb > 0",
      "not op_mob > 0",
      "not op_mini all",
      "not ios_saf < 13"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version"
    ]
  }
}
